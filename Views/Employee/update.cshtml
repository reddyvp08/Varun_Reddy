@using emp;
@model Employee;

<h2>update EMPLOYEE</h2>
<hr/>
@{
    int e=ViewBag.update;
}
<form method="post" action="/Employee/updatee">
  <div class="form-group">
    <label for="eid">EMPID:</label>
    <input type="text" id="eid" class="form-control" value=@e asp-for="eid">
  </div>
  <div class="form-group">
    <label for="ename">EMPNAME:</label>
    <input type="text" id="ename" class="form-control" asp-for="ename">
  </div>
  <div class="form-group">
    <label for="edept">EDEPT:</label>
    <input type="text" id="edept" class="form-control"  asp-for="edept">
  </div>
 <div class="form-group">
   <button type="submit" class="btn btn-danger">Submit</button>
  </div>
 
</form>